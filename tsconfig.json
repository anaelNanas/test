{
  "compilerOptions": {

    "outDir": "./built",
    "target": "es2016",                      
    "jsx": "preserve",                       

    "sourceMap": false,
    "module": "commonjs",              

    "allowJs": true,                         
    "esModuleInterop": true,                 

    "strict": false,                         

    "skipLibCheck": true                     
  },
  "exclude": ["./built/**/*"] // This is what fixed it!
}

